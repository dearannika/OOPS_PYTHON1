    "class Animal:\n",
    "    species = \"Mammal\"  # Class attribute\n",
    "\n",
    "    def __init__(self, name):\n",
    "        self.name = name  # Instance attribute\n",
    "\n",
    "dog = Animal(\"Dog\")\n",
    "cat = Animal(\"Cat\")\n",
    "\n",
    "print(dog.name, dog.species)  # Output: Dog Mammal\n",
    "print(cat.name, cat.species)  # Output: Cat Mammal\n"
   ]
