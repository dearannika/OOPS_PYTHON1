   "source": [
    "#Method Overloading Using *args\n",
    "class Calculator:\n",
    "    def add(self, *args):\n",
    "        return sum(args)\n",
    "\n",
    "calc = Calculator()\n",
    "print(calc.add(1, 2))  # Output: 3\n",
    "print(calc.add(1, 2, 3, 4))  # Output: 10\n"
   ]
  },
